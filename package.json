{
  "name": "java-parser",
  "version": "0.0.2",
  "description": "Java1.15 parser",
  "scripts": {
    "build": "node_modules/pegjs/bin/pegjs -o lib/javaparser15.js --format umd src/Java.1.15.pegjs",
    "build-min": "npm run build && node_modules/uglify-js/bin/uglifyjs lib/javaparser15.js -o lib/javaparser15.min.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/algomorph/jsjavaparser.git"
  },
  "keywords": [
    "java",
    "parser"
  ],
  "author": "Oleg Mazko",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/algomorph/jsjavaparser/issues"
  },
  "main": "javaparser15.js",
  "bin": {
    "java-parser": "cmd.js"
  },
  "devDependencies": {
    "pegjs": "^0.10.0",
    "uglify-js": "^2.8.29",
    "umd": "^3.0.1"
  },
  "dependencies": {
    "cli": "^1.0.1",
    "pegjs": "^0.10.0",
    "uglify-js": "^3.13.1"
  }
}
